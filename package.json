{
	"name": "flxc",
	"version": "0.2.0",
	"description": "Flatten and run multi-line executable shell script file.",
	"main": "flxc.js",
	"option": {
		"shell": "flxc"
	},
	"bin": {
		"flxc": "run.js"
	},
	"scripts": {
		"formal": "jesy apply '*.json'",
		"clear": "rsetmod clean --clear",
		"reset": "rsetmod clean",
		"load": "flxc execute ./.install.sh",
		"pristine": "npm run reset && npm run load && npm run formal",
		"test": "mocha flxc-test.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/volkovasystems/flxc.git"
	},
	"keywords": [
		"flatten",
		"run",
		"multi-line",
		"executable",
		"shell",
		"script",
		"file",
		"flxc"
	],
	"author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/volkovasystems/flxc/issues"
	},
	"homepage": "https://github.com/volkovasystems/flxc#readme",
	"dependencies": {
		"depher": "^0.38.0",
		"detr": "^0.4.0",
		"falzy": "^0.23.0",
		"gnaw": "^0.21.0",
		"kept": "^0.24.0",
		"lire": "^0.17.0",
		"pedon": "^0.15.0",
		"protype": "^0.24.0",
		"shft": "^0.5.0",
		"yargs": "^9.0.1"
	}
}